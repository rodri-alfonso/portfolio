---
import ProjectCard from 'components/ProjectCard.astro'
import Section from '../layouts/Section.astro'
import { PROJECTS_LIST } from '../config'
---

<Section sectionId='projects' title='Featured Projects' class='gap-12'>
	<div class='grid md:grid-cols-3 grid-cols-1 sm:grid-cols-2 gap-6'>
		{PROJECTS_LIST.map((project) => <ProjectCard {...project} />)}
	</div>
</Section>
